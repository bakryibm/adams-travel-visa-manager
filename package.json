{
  "name": "adams-travel-visa-manager",
  "private": true,
  "version": "1.0.0",
  "main": "main.cjs",
  "description": "تطبيق سطح مكتب لإدارة التأشيرات والسفر مبني بـ React و TypeScript",
  "author": "Adams Travel",
  "homepage": "https://github.com/adams-travel/adams-travel-visa-manager#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adams-travel/adams-travel-visa-manager.git"
  },
  "bugs": {
    "url": "https://github.com/adams-travel/adams-travel-visa-manager/issues"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"npm run electron\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "desktop": "electron main.js",
    "desktop:win": "electron-packager . --platform=win32 --arch=x64 --out=release"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "date-fns": "^3.3.1",
    "jsqr": "^1.4.0",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.6"
  },
  "build": {
    "appId": "com.adams.travelvisamanager",
    "productName": "Adams Travel Visa Manager",
    "copyright": "Copyright © 2025 Adams Travel Visa Manager",
    "directories": {
      "output": "dest"
    },
    "win": {
      "target": "portable",
      "signAndEditExecutable": false,
      "verifyUpdateCodeSignature": false
    },
    "extraResources": [
      "dist/**/*",
      "main.js"
    ]
  }
}
